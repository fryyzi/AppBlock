<Window x:Class="BLockGame.UnBlock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BLockGame"
        mc:Ignorable="d"
        Title="UnBlock" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="DataTextBox" HorizontalAlignment="Left" Height="183" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="790" Background="#FFF0C7C7"/>
        <Button x:Name="UpdateButton" Content="Оновити" FontSize="30" HorizontalAlignment="Left" Margin="10,362,0,0" VerticalAlignment="Top" Height="62" Width="168" Click="UpdateButton_Click"/>
        <Button Content="Видалити" FontSize="30" HorizontalAlignment="Left" Margin="622,362,0,0" VerticalAlignment="Top" Height="62" Width="168" Click="Button_Click"/>
        <TextBox x:Name="DeteleAppTextBox" HorizontalAlignment="Left" Margin="56,262,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="678" Height="54"/>
        <TextBlock HorizontalAlignment="Left" Margin="94,217,0,0" TextWrapping="Wrap" Text="Вкажіть назву програми яку хочете видалити" FontSize="30" VerticalAlignment="Top"/>
    </Grid>
</Window>
